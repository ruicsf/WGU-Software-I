<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ModifyPartController">
   <children>
      <VBox prefHeight="400.0" prefWidth="593.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="593.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                     <children>
                        <Label text="Modify Part">
                           <HBox.margin>
                              <Insets left="20.0" top="30.0" />
                           </HBox.margin>
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                        <RadioButton fx:id="modifyInHouseRadio" mnemonicParsing="false" onAction="#onInHouseModify" selected="true" text="In-House">
                           <HBox.margin>
                              <Insets left="20.0" top="30.0" />
                           </HBox.margin>
                           <toggleGroup>
                              <ToggleGroup fx:id="partTG" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="modifyOutsourcedRadio" mnemonicParsing="false" onAction="#onOutsourcedModify" text="Outsourced" toggleGroup="$partTG">
                           <HBox.margin>
                              <Insets left="20.0" top="30.0" />
                           </HBox.margin>
                        </RadioButton>
                     </children>
                  </HBox>
                  <GridPane>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="129.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="326.0" minWidth="10.0" prefWidth="140.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="334.0" minWidth="10.0" prefWidth="308.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Label fx:id="PartIdModify" text="ID">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label fx:id="PartNameModify" text="Name" GridPane.rowIndex="1">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label fx:id="PartInvModify" text="Inv" GridPane.rowIndex="2">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label fx:id="PartPriceModify" text="Price/Cost" GridPane.rowIndex="3">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label fx:id="PartMaxModify" text="Max" GridPane.rowIndex="4">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label fx:id="PartMachineIDModify" text="Machine ID" GridPane.rowIndex="5">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <TextField fx:id="idTxtModify" disable="true" editable="false" GridPane.columnIndex="1" />
                        <TextField fx:id="partNameTxtModify" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="partInvTxtModify" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField fx:id="partPriceTxtModify" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <TextField fx:id="partMaxTxtModify" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <TextField fx:id="partDynamicTxtModify" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <TextField fx:id="MPfieldIndex" editable="false" visible="false" />
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                           <children>
                              <Label fx:id="PartMinModify" text="Min" />
                              <TextField fx:id="partMinTxtModify" />
                           </children>
                           <GridPane.margin>
                              <Insets left="15.0" />
                           </GridPane.margin>
                        </HBox>
                     </children>
                  </GridPane>
                  <GridPane prefHeight="83.0" prefWidth="593.0">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="287.0" minWidth="10.0" prefWidth="287.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="10.0" prefWidth="79.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="277.0" minWidth="10.0" prefWidth="227.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Button mnemonicParsing="false" onAction="#partOnSaveBtnModify" prefHeight="25.0" prefWidth="55.0" text="Save" GridPane.columnIndex="1" />
                        <Button mnemonicParsing="false" onAction="#toMainMenuBtn" prefHeight="25.0" prefWidth="55.0" text="Cancel" GridPane.columnIndex="2" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
